{
  "name": "vtss",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "cal:auth": "dotenvx run -- npx tsx src/node/gcal_write_creds.ts",
    "cal:pull": "dotenvx run -- npx tsx src/node/gcal_write_events.ts",
    "dev": "dotenvx run -- astro dev",
    "dev:network": "dotenvx run -- astro dev --host",
    "build": "dotenvx run -- astro check && astro build",
    "preview": "dotenvx run -- astro preview",
    "preview:network": "dotenvx run -- astro preview --host",
    "astro": "dotenvx run -- astro",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/markdoc": "^0.11.5",
    "@astrojs/mdx": "^3.1.8",
    "@astrojs/netlify": "^5.5.3",
    "@astrojs/react": "^3.6.2",
    "@astrojs/rss": "^4.0.8",
    "@astrojs/sitemap": "^3.2.0",
    "@astrojs/tailwind": "^5.1.2",
    "@fontsource/inter": "^5.1.0",
    "@fontsource/lora": "^5.1.0",
    "@keystatic/astro": "^5.0.1",
    "@keystatic/core": "^0.5.37",
    "@tailwindcss/typography": "^0.5.15",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "astro": "^4.16.2",
    "astro-embed": "^0.7.4",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "eslint": "^9.12.0",
    "eslint-plugin-astro": "^1.2.4",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sharp": "^0.33.5",
    "tailwind-merge": "^2.5.3",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.3"
  },
  "packageManager": "pnpm@9.4.0",
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.19.1",
    "@google-cloud/local-auth": "^3.0.1",
    "googleapis": "^144.0.0",
    "js-yaml": "^4.1.0",
    "tsx": "^4.19.1",
    "turndown": "^7.2.0"
  }
}
